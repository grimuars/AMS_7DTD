	<set xpath="//block[@name='ConfigFeatureBlock']/property[@class='AdvancedPlayerFeatures']/property[@name='Logging']/@value">false</set>
	<!--for debugging-->
	<set xpath="//block[@name='ConfigFeatureBlock']/property[@class='AdvancedPlayerFeatures']/property[@name='Encumbrance']/@value">true</set>
	<set xpath="//block[@name='ConfigFeatureBlock']/property[@class='AdvancedPlayerFeatures']/property[@name='Encumbrance_ToolBelt']/@value">true</set>
