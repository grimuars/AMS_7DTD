<ByHowlune>
	<!--Stronger stability means less potential stability calculation and fewer collapses.-->
	<set xpath="/materials/material/property[@name='stability_glue' and @value='4']/@value">20</set>
	<set xpath="/materials/material/property[@name='stability_glue' and @value='20']/@value">40</set>
	<set xpath="/materials/material/property[@name='stability_glue' and @value='40']/@value">80</set>
	<set xpath="/materials/material/property[@name='stability_glue' and @value='120']/@value">240</set>
	<set xpath="/materials/material/property[@name='stability_glue' and @value='300']/@value">600</set>
</ByHowlune>	


