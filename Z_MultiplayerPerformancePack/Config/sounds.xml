	<set xpath="/Sounds/SoundDataNode[starts-with(@name, 'cement_mixer')]/MaxRepeatRate/@value">0.1</set>
	<!--Hamfisted into some weapons, but dont even play properly due to playing them the exact same time the weapon fires.-->
	<remove xpath="/Sounds/SoundDataNode[@name='bullet_shell_eject']"/>
	<remove xpath="/Sounds/SoundDataNode[@name='shotgun_shell_eject']"/>
	<!--Fix wrong sound paths.-->
	<set xpath="/Sounds/SoundDataNode[@name='snakepain']/AudioClip[@ClipName='Sounds/Animals/Snake/snakepain1']/@ClipName">Sounds/Animals/Snake/snake_pain1</set>
	<set xpath="/Sounds/SoundDataNode[@name='snakepain']/AudioClip[@ClipName='Sounds/Animals/Snake/snakepain2']/@ClipName">Sounds/Animals/Snake/snake_pain2</set>
