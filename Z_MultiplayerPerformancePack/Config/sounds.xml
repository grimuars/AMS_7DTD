	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_VO_Announce']">
		<distantfadeend name="100"/>
	</insertAfter>
	<append xpath="/Sounds/SoundDataNode[@name='auger_idle']">
		<distantfadeend name="100"/>
	</append>
	<append xpath="/Sounds/SoundDataNode[@name='silencefiller']">
		<distantfadeend name="100"/>
	</append>
	<append xpath="/Sounds/SoundDataNode[contains(@name,'_s_')]">
		<distantfadeend name="150"/>
	</append>
	<!--Reduce sound spam.-->
	<set xpath="/Sounds/SoundDataNode[not (contains(@name, '_fire'))]/MaxRepeatRate[@value='0.001']/@value">0.01</set>
	<set xpath="/Sounds/SoundDataNode[contains(@name, 'hitcloth')]/MaxRepeatRate/@value">0.5</set>
	<set xpath="/Sounds/SoundDataNode[contains(@name, 'hitcloth')]/MaxVoices/@value">3</set>
