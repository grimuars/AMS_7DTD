	</insertAfter>
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_Creatures100Meter']">
		<distantfadeend name="100"/>
	</insertAfter>
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_Creatures']">
		<distantfadeend name="20"/>
	</insertAfter>
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_CreatureSenseAlert']">
		<distantfadeend name="30"/>
	</insertAfter>
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_CreaturesLG']">
		<distantfadeend name="40"/>
	</insertAfter>
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_Creatures100Meter']">
		<distantfadeend name="100"/>
	</insertAfter>
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_Vehicle']">
