<ByHowlune>
	<!--Range filtration. Masively reduce audio netpackages, cutoff based on audiosource range. In order of most used.-->
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_Footsteps']">
		<distantfadeend name="30"/>
	</insertAfter>
	<!--<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_WeaponFire']"><distantfadeend name="250"/></insertAfter>-->
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[starts-with(@name, 'Sounds/AudioSource_Player')]">
		<distantfadeend name="50"/>
	</insertAfter>
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[starts-with(@name, 'Sounds/AudioSource_Pain')]">
		<distantfadeend name="75"/>
	</insertAfter>
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_Impact']">
		<distantfadeend name="50"/>
	</insertAfter>
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_Impact_Large']">
		<distantfadeend name="50"/>
	</insertAfter>
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_Impact_Small']">
		<distantfadeend name="25"/>
	</insertAfter>
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_Impact_XSmall']">
		<distantfadeend name="13"/>
	</insertAfter>
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_Melee']">
		<distantfadeend name="25"/>
	</insertAfter>
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_Bows']">
		<distantfadeend name="25"/>
	</insertAfter>
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_Reloads']">
		<distantfadeend name="10"/>
	</insertAfter>
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_ReloadsFullVolume']">
		<distantfadeend name="10"/>
	</insertAfter>
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_Interact']">
		<distantfadeend name="20"/>
	</insertAfter>
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_Interact_Large']">
		<distantfadeend name="50"/>
	</insertAfter>
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_InteractFullVolume']">
		<distantfadeend name="25"/>
	</insertAfter>
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_ZPack']">
		<distantfadeend name="10"/>
	</insertAfter>
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_HandTool']">
		<distantfadeend name="30"/>
	</insertAfter>
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/Weapons/Motorized/Auger/AugerAudioSource']">
