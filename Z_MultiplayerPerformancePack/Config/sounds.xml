	</insertAfter> -->
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_UI']">
		<distantfadeend name="25"/>
	</insertAfter>
	<insertAfter xpath="/Sounds/SoundDataNode/AudioSource[@name='Sounds/AudioSource_Notifications']">
