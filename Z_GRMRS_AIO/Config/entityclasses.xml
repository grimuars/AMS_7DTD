	<insertAfter xpath="/entity_classes/entity_class[@name='playerMale']/effect_group/passive_effect[@name='PlayerExpGain' and @tags='Upgrading']">
		<!-- До 50 уровня 10% опыта от торговли -->
		<passive_effect name="PlayerExpGain" operation="perc_add" value="-0.9" tags="Selling">
			<requirement name="PlayerLevel" operation="LT" value="50"/>
		</passive_effect>
		<!-- С 50 уровня 70% опыта от торговли -->
		<passive_effect name="PlayerExpGain" operation="perc_add" value="-0.3" tags="Selling">
			<requirement name="PlayerLevel" operation="GTE" value="50"/>
		</passive_effect>
		<!-- До 50 уровня 30% опыта от добычи -->
		<passive_effect name="PlayerExpGain" operation="perc_add" value="-0.7" tags="Harvesting">
			<requirement name="PlayerLevel" operation="LT" value="50"/>
		</passive_effect>
		<!-- До 50 уровня в 3 раза больше опыта от убийств -->
		<passive_effect name="PlayerExpGain" operation="perc_add" value="3" tags="Kill">
			<requirement name="PlayerLevel" operation="LT" value="50"/>
		</passive_effect>
		<!-- С 75 уровня - в 3 раза больше опыта за квесты -->
		<passive_effect name="PlayerExpGain" operation="perc_add" value="3" tags="Quest">
			<requirement name="PlayerLevel" operation="GTE" value="75"/>
		</passive_effect>
	</insertAfter>
